#include <stdio.h>
#include <stdlib.h>
int main()
{
    int n;
    int sum=0;
    scanf("%d", &n);
    for(int i=1; i<=n; i++)
    {
        if(n%i==0)
        {
            sum+=i;
        }
    }
    if(sum==2*n)
    {
        printf("%d la so hoan hao", n);
    }
    else
    {
        printf("%d khong phai so hoan hao", n);
    }
    return 0;
}

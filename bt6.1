#include <stdio.h>
#include <stdlib.h>
int main()
{
    int a, b, r;
    scanf("%d %d", &a, &b);
    int temp_a = a;
    int temp_b = b;
    while (b!=0)
    {
        r=a%b;
        a=b;
        b=r;
    }
    int gcd = a;
    int lcm = (temp_a * temp_b) / gcd;
    printf("gcd = %d\n", gcd);
    printf("lcm = %d\n", lcm);
    return 0;
}
